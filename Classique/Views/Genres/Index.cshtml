@model IEnumerable<Classique.Models.Genre>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<table class="table">
    <tr>
        
          <h2>LISTE DES GENRES</h2>  
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Libelle_Abrege)
        </td>
        <td>
            
            @Html.ActionLink("Albums correspondants", "Details", new { id=item.Code_Genre })
            
        </td>
    </tr>
}

</table>
